{
  "name": "netli.fyi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit --skipLibCheck && eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "pod": "cd ios/ && rm -rf Pods/ && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "env": "yarn rnuc .env",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean && cd ../",
    "clean:node": "rm -rf node_modules && yarn",
    "clean": "yarn clean:ios && yarn clean:node && yarn pod && yarn env",
    "owl:build:ios": "yarn owl build --platform ios",
    "owl:test:ios": "yarn owl test --platform ios",
    "owl:build:android": "yarn owl build --platform android",
    "owl:test:android": "yarn owl test --platform android",
    "x": "cd ios/ && xed . &&  cd ..",
    "generate-icon": "yarn react-native generate-bootsplash src/assets/images/app-icon.png     --background-color=F5FCFF     --logo-width=100     --assets-path=assets     --flavor=main",
    "np": "np --no-publish",
    "postversion": "react-native-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plahteenlahti/netli.fyi-app.git"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.17",
    "@react-navigation/native": "^6.1.15",
    "@react-navigation/native-stack": "^6.9.23",
    "@react-navigation/stack": "^6.3.26",
    "@reduxjs/toolkit": "^2.2.1",
    "@tanstack/react-query": "^5.25.0",
    "@types/qs": "^6.9.12",
    "date-fns": "^3.3.1",
    "i18n-js": "^4.4.3",
    "lodash": "^4.17.21",
    "nativewind": "^2.0.11",
    "qs": "^6.12.0",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-app-auth": "^7.1.3",
    "react-native-bootsplash": "^5.4.1",
    "react-native-device-info": "^10.13.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.12.1",
    "react-native-purchases": "^7.23.0",
    "react-native-reanimated": "^3.8.0",
    "react-native-redash": "^18.1.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "^15.1.0",
    "react-native-ultimate-config": "^6.0.1",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/i18n-js": "^3.8.4",
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.71.3",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "np": "^7.6.2",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "react-native-owl": "^1.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "typescript-styled-plugin": "^0.18.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
