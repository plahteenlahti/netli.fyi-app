{
  "name": "netli.fyi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit --skipLibCheck && eslint . --ext .js,.jsx,.ts,.tsx",
    "pod": "cd ios/ && rm -rf Pods/ && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "env": "yarn rnuc .env",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean && cd ../",
    "clean:node": "rm -rf node_modules && yarn",
    "clean": "yarn clean:ios && yarn clean:node && yarn pod && yarn env",
    "x": "cd ios/ && xed . &&  cd ..",
    "generate-icon": "yarn react-native generate-bootsplash src/assets/images/app-icon.png     --background-color=F5FCFF     --logo-width=100     --assets-path=assets     --flavor=main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plahteenlahti/netli.fyi-app.git"
  },
