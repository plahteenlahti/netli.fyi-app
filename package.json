{
  "name": "deploy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit --skipLibCheck && eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "pod": "cd ios/ && rm -rf Pods/ && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "env": "yarn rnuc .env",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean && cd ../",
    "clean:node": "rm -rf node_modules && yarn",
    "clean": "yarn clean:ios && yarn clean:node && yarn pod && yarn env",
    "x": "cd ios/ && xed . &&  cd ..",
    "generate-icon": "yarn react-native generate-bootsplash src/assets/images/app-icon.png     --background-color=F5FCFF     --logo-width=100     --assets-path=assets     --flavor=main"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/stack": "^6.2.2",
    "@reduxjs/toolkit": "^1.8.3",
    "date-fns": "^2.28.0",
    "i18n-js": "^3.9.2",
    "lodash": "^4.17.21",
    "react": "18.0.0",
    "react-native": "0.69.4",
    "react-native-app-auth": "^6.4.3",
    "react-native-bootsplash": "^4.2.3",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-lumi": "^0.1.1",
    "react-native-mmkv": "^2.4.3",
    "react-native-reanimated": "^2.9.1",
    "react-native-redash": "^18.0.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-svg": "^12.4.0",
    "react-native-ultimate-config": "^4.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/react-native": "^0.64.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^25.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-native-clean-project": "^3.4.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.2.3",
    "typescript-styled-plugin": "^0.15.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
