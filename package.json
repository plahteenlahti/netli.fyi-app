{
  "name": "netli.fyi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "tsc --noEmit --skipLibCheck && eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "pod": "cd ios/ && rm -rf Pods/ && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "env": "yarn rnuc .env",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean && cd ../",
    "clean:node": "rm -rf node_modules && yarn",
    "clean": "yarn clean:ios && yarn clean:node && yarn pod && yarn env",
    "owl:build:ios": "yarn owl build --platform ios",
    "owl:test:ios": "yarn owl test --platform ios",
    "owl:build:android": "yarn owl build --platform android",
    "owl:test:android": "yarn owl test --platform android",
    "x": "cd ios/ && xed . &&  cd ..",
    "generate-icon": "yarn react-native generate-bootsplash src/assets/images/app-icon.png     --background-color=F5FCFF     --logo-width=100     --assets-path=assets     --flavor=main",
    "np": "np --no-publish",
    "postversion": "react-native-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plahteenlahti/netli.fyi-app.git"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-firebase/analytics": "^16.4.5",
    "@react-native-firebase/app": "^16.4.5",
    "@react-native-firebase/crashlytics": "^16.4.5",
    "@react-native-firebase/remote-config": "^16.4.5",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@react-navigation/stack": "^6.3.4",
    "@reduxjs/toolkit": "^1.9.0",
    "@tanstack/react-query": "^4.16.1",
    "@types/qs": "^6.9.7",
    "date-fns": "^2.29.3",
    "i18n-js": "^4.1.1",
    "lodash": "^4.17.21",
    "qs": "^6.11.0",
    "react": "18.0.0",
    "react-native": "0.69.4",
    "react-native-app-auth": "^6.4.3",
    "react-native-bootsplash": "^4.3.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-lumi": "^0.1.1",
    "react-native-mmkv": "^2.5.1",
    "react-native-purchases": "^5.2.2",
    "react-native-reanimated": "^2.13.0",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.6.0",
    "react-native-ultimate-config": "^4.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/i18n-js": "^3.8.0",
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.189",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-jest": "^25.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "np": "^7.6.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-native-owl": "^1.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.9.3",
    "typescript-styled-plugin": "^0.18.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
